{
  "env": {
    "FIREBASE_KEY": "@firebase-sdk-key",
    "FIREBASE_CLIENT_EMAIL": "@firebase-sdk-client-email",
    "FIREBASE_PROJECT_ID": "@firebase-sdk-project-id",
    "FIREBASE_DATABASE_URL": "@firebase-sdk-db-url",
    "CLIENT_SECRET": "@client-secret",
    "CLIENT_ID": "@client-id"
  },
  "build": {
    "env": {
      "PORT": "@port",
      "REACT_APP_FIREBASE_API_KEY": "@firebase-api-key",
      "REACT_APP_AUTH_DOMAIN": "@firebase-auth-domain",
      "REACT_APP_DB_URL": "@firebase-app-url",
      "REACT_APP_PROJECT_ID": "@firebase-project-id",
      "REACT_APP_STORAGE_BUCKET": "@firebase-storage-bucket",
      "REACT_APP_MESSAGING_SENDER_ID": "@firebase-messaging-sender-id",
      "REACT_APP_APP_ID": "@firebase-app-id",
    }
}